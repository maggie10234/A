
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <div>訂單編號</div>
        <div>@Html.TextBox("txtOrderId")</div>
        <table>
            <tr>
                <td>客戶代號</td>
                <td>客戶名稱</td>
                <td>訂單日期</td>
            </tr>
            @foreach (var item in (List<eSaleX.Models.Order>)ViewBag.Data)
             {
                <tr>
                     <td>@item.CustID</td>
                     <td>@item.CustName</td>
                     <td>@item.OrderDate</td>
                </tr>
             }
        </table>
    </div>
</body>
</html>
